<!-- [ breadcrumb ] start -->
<div class="page-header">
  <div class="page-block">
    <div class="row align-items-center">
      <div class="col-md-12">
        <ul class="breadcrumb">
          <li class="breadcrumb-item"><a href="@Url.Action("Index", "Dashboard")">Home</a></li>
          <li class="breadcrumb-item"><a href="javascript: void(0)">@ViewBag.BreadcrumItem</a></li>
          <li class="breadcrumb-item" aria-current="page">@ViewBag.BreadcrumbItemActive</li>
        </ul>
      </div>
      <div class="col-md-12">
        <div class="page-header-title">
          <h2 class="mb-0">@ViewBag.BreadcrumbItemActive</h2>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- [ breadcrumb ] end -->
