<script>
    document.addEventListener('DOMContentLoaded', function () {
        if('@TempData["DarkLayout"]' == "default"){
            if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
                dark_layout = "true";
            } else {
                dark_layout = 'false';
            }
            layout_change_default();
            if (dark_layout == 'true') {
                layout_change('dark');
            } else {
                layout_change('light');
            }
        }
        if('@TempData["DarkLayout"]' != "default"){
            if('@TempData["DarkLayout"]' == "true"){
                layout_change('dark');
            }else{
                layout_change('light');
            }
        }
        if('@TempData["ThemeContrast"]' == "true"){
            layout_theme_contrast_change('true');
        }else{
            layout_theme_contrast_change('false');
        }
        if('@TempData["BoxContainer"]' == "true"){
            change_box_container('true');
        }else{
            change_box_container('false');
        }
        if('@TempData["CaptionShow"]' == "true"){
            layout_caption_change('true');
        }else{
            layout_caption_change('false');
        }
        if('@TempData["RtlLayout"]' == "true"){
            layout_rtl_change('true');
        }else{
            layout_rtl_change('false');
        }
        if('@TempData["PresetTheme"]' != ""){
            preset_change('@TempData["PresetTheme"]');
        }
    });
</script>